{
  "version": 2,
  "routes": [
    { "src": "^/$", "dest": "/api/configure" },
    { "src": "^/configure$", "dest": "/api/configure" },
    { "src": "^/download/([^/]+)/([^/]+)$", "dest": "/api/download?apiKey=$1&data=$2" },
    { "src": "^/([^/]+)/manifest.json$", "dest": "/api/manifest?apiKey=$1" },
    { "src": "^/([^/]+)/stream/(movie|series)/(.+).json$", "dest": "/api/stream?apiKey=$1&type=$2&id=$3" }
  ]
}
